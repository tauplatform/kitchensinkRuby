<div class="container-fluid">

  <%= render :partial => "/partials/page_head", :locals => {:back => ".", :title => "Battery.properties"} %>



  <div class="list-group">

    <div class="list-group-item">
      <strong>averageCurrentConsumption</strong>
      <p class="item-value">
        <% begin %>
            <%= Rho::Battery.averageCurrentConsumption %>
        <% rescue Exception => e %>
            <%= e.message %>
        <% end %>
      </p>
    </div>

    <div class="list-group-item">
      <strong>refreshInterval</strong>

      <p class="item-value">
        <% begin %>
            <%= Rho::Battery.refreshInterval %>
        <% rescue Exception => e %>
            <%= e.message %>
        <% end %>
      </p>
    </div>

    <div class="list-group-item">
      <strong>tripDuration</strong>

      <p class="item-value">
        <% begin %>
            <%= Rho::Battery.tripDuration %>
        <% rescue Exception => e %>
            <%= e.message %>
        <% end %>
      </p>
    </div>

  </div>


</div>